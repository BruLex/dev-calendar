{
  "manifest_version": 2,
  "name": "Dev calendar",
  "description": "Zones calendar",
  "version": "1.1",
  "browser_action": {
    "default_icon": "calendar.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
